{
  "name": "cypress",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/**/*.js",
    "nonGlobalStepDefinitions": false,
    "tags": "not @ignore"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:headed": "cypress run --headed",
    "cy:run:spec": "cypress run --spec cypress/e2e/first-test.cy.js",
    "cy:open:dev": "cypress open --env configFile=dev",
    "cy:open:staging": "cypress open --env configFile=staging",
    "cy:run:smoke": "cypress run --spec 'cypress/e2e/smoke/**/*.cy.js'",
    "cy:run:regression": "cypress run --spec 'cypress/e2e/regression/**/*.cy.js'",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "test:dev": "cypress run --env configFile=dev",
    "test:staging": "cypress run --env configFile=staging",
    "test:prod": "cypress run --env configFile=prod --spec 'cypress/e2e/smoke/**/*.cy.js'",
    "test:report": "cypress run --reporter mochawesome",
    "report:generate": "mochawesome-merge cypress/reports/*.json > cypress/reports/report.json && marge cypress/reports/report.json -f index -o cypress/reports"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.6",
    "@faker-js/faker": "^10.0.0",
    "cypress": "^14.5.4",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-xpath": "^2.0.1",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.3.0"
  }
}
